.container {
    display: flex;
    flex-flow: nowrap column;
    height: 90%;
    align-content: stretch;
}

.topics {
    display: flex;
    flex-flow: nowrap column;
    width: 91%;
    height: 120%;
    margin-top: -20%;
    background-color: transparent;
}

.topics .item{    
    margin: 10px 0;
    background-color: transparent;
    border-radius: 10px;
    border-style: solid;
    border-width: 0.5px;
    border-color: #f7c98e;
}

.cell {
    position: absolute;
    z-index: 1;
    background-color:transparent;
    margin: 10px 20px;
    height: auto;
}

.cell-label {
    display:flex;
    text-align: left;
    vertical-align: top;
    font-size: 12px;
    font-weight:bolder;
}

.cell-content {
    display:flex;
    flex-direction: row;
    vertical-align: center;
    font-size: 12px;
    font-weight: lighter;
}

.img {
    position:relative;
    margin-left: 92%;
    margin-top: 20rpx;
    margin-bottom: 1rpx;
    z-index: 2;
    background-color: red;
    display:flex;
    flex-direction: row row-reverse;
    height: 100px;
    width: 0px;
}

.img image{
    height: 64px;
    width: 64px;
}

.img t-icon{
  height: 64px;
  width: 64px;
}

.header {
    font-size: 24px;
    margin: 20px 0;
}

.footer {
    display: flex;
    justify-content: space-around;
    width: 100%;
    position: fixed;
    bottom: 0;
    background-color: #fff;
    padding: 10px 0;
}

